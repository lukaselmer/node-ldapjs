{
  "originalAuthor": "Mark Cavage <mcavage@gmail.com>",
  "name": "my-webpack-project",
  "homepage": "http://ldapjs.org",
  "description": "My webpack project",
  "version": "1.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/ldapjs/node-ldapjs.git"
  },
  "main": "lib/index.js",
  "directories": {
    "lib": "./lib"
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "dependencies": {
    "abstract-logging": "^2.0.0",
    "asn1": "^0.2.4",
    "assert-plus": "^1.0.0",
    "backoff": "^2.5.0",
    "ldap-filter": "^0.3.3",
    "once": "^1.4.0",
    "vasync": "^2.2.0",
    "verror": "^1.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.18.2",
    "@webpack-cli/generators": "^3.0.1",
    "babel-loader": "^8.2.5",
    "eslint": "^7.20.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "front-matter": "^4.0.2",
    "get-port": "^5.1.1",
    "highlight.js": "^11.0.1",
    "husky": "^4.2.5",
    "marked": "^4.0.0",
    "tap": "15.2.3",
    "tap-webpack-plugin": "^2.0.0",
    "terser-webpack-plugin": "^5.3.6",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "scripts": {
    "test": "tap --no-cov -R terse",
    "test:ci": "tap --coverage-report=lcovonly -R terse",
    "test:cov": "tap -R terse",
    "test:cov:html": "tap --coverage-report=html -R terse",
    "test:minified": "webpack && npm run test",
    "test:minified2": "webpack && node build/test/client.test.js",
    "test:watch": "tap -n -w --no-coverage-report -R terse",
    "test:integration": "tap --no-cov -R terse 'test-integration/**/*.test.js'",
    "test:integration:local": "docker-compose up -d && npm run test:integration && docker-compose down",
    "lint": "eslint . --fix",
    "lint:ci": "eslint .",
    "docs": "node scripts/build-docs.js",
    "build": "webpack --mode=production --node-env=production",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "watch": "webpack --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:ci && npm run test"
    }
  }
}
